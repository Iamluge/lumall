<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">


<struts>


	<package name="proInfoIndex" namespace="/proInfoIndex" extends="mypackage">
		
		
		<action name="settingIndexToTemplate" class="proInfoIndexAction"
			method="settingIndexToTemplate">
			<result type="json">
				<param name="root">extMessage</param>
			</result>
		</action>
		
		<action name="enterIndexValue" class="proInfoIndexAction"
			method="enterIndexValue">
			<result type="json">
				<param name="root">extMessage</param>
			</result>
		</action>
		
		<action name="editTemplateIndex" class="proInfoIndexAction"
			method="editTemplateIndex">
			<result type="json">
				<param name="root">extMessage</param>
			</result>
		</action>
		
		<action name="findIndexByTemplateId" class="proInfoIndexAction"
			method="findIndexByTemplateId">
			<result type="json">
				<param name="root">proInfoIndexList</param>
			</result>
		</action>
		
		<action name="findIndexValueByTemplateId" class="proInfoIndexAction"
			method="findIndexValueByTemplateId">
			<result type="json">
				<param name="root">indexValueList</param>
			</result>
		</action>
		
		<action name="findIndexValueByTemAndProInfo" class="proInfoIndexAction"
			method="findIndexValueByTemAndProInfo">
			<result type="json">
				<param name="root">indexValueList</param>
			</result>
		</action>
	</package>

</struts>